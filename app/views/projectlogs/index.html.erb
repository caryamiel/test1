<p id="notice"><%= notice %></p>

<h1>Listing Projectlogs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Action</th>
      <th>Parameter</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projectlogs.each do |projectlog| %>
      <tr>
        <td><%= projectlog.user_id %></td>
        <td><%= projectlog.action %></td>
        <td><%= projectlog.parameter %></td>
        <td><%= projectlog.project_id %></td>
        <td><%= link_to 'Show', projectlog %></td>
        <td><%= link_to 'Edit', edit_projectlog_path(projectlog) %></td>
        <td><%= link_to 'Destroy', projectlog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Projectlog', new_projectlog_path %>
