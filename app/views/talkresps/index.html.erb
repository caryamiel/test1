<p id="notice"><%= notice %></p>

<h1>Listing Talkresps</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Projtalk</th>
      <th>Talkcontent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @talkresps.each do |talkresp| %>
      <tr>
        <td><%= talkresp.user_id %></td>
        <td><%= talkresp.projtalk_id %></td>
        <td><%= talkresp.talkcontent %></td>
        <td><%= link_to 'Show', talkresp %></td>
        <td><%= link_to 'Edit', edit_talkresp_path(talkresp) %></td>
        <td><%= link_to 'Destroy', talkresp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Talkresp', new_talkresp_path %>
