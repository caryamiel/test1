<p id="notice"><%= notice %></p>

<h1>Listing Projtalks</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Project</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projtalks.each do |projtalk| %>
      <tr>
        <td><%= projtalk.user_id %></td>
        <td><%= projtalk.project_id %></td>
        <td><%= projtalk.name %></td>
        <td><%= link_to 'Show', projtalk %></td>
        <td><%= link_to 'Edit', edit_projtalk_path(projtalk) %></td>
        <td><%= link_to 'Destroy', projtalk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Projtalk', new_projtalk_path %>
